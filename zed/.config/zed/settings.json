{
  "assistant": {
    "default_model": {
      "model": "claude-3-5-sonnet-latest",
      "provider": "zed.dev"
    },
    "dock": "right",
    "version": "2"
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "MonoLisa Custom",
  "buffer_font_size": 13, // 14 on chaos
  "buffer_font_weight": 500,
  "buffer_line_height": "comfortable",
  "cursor_blink": false,
  "cursor_shape": "block",
  "features": {
    "inline_completion_provider": "supermaven"
  },
  "journal": {
    "hour_format": "hour24"
  },
  "indent_guides": {
    "active_line_width": 1,
    "background_coloring": "disabled",
    "coloring": "indent_aware",
    "enabled": false,
    "line_width": 1
  },
  "preferred_line_length": 80,
  "project_panel": {
    "default_width": 320,
    "dock": "left",
    "indent_guides": {
      "show": "never"
    }
  },
  "show_whitespaces": "all",
  "show_wrap_guides": true,
  "soft_wrap": "preferred_line_length",
  "tab_size": 2,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "default_height": 320,
    "detect_venv": {
      "on": {
        "directories": [".venv", "venv"],
        "activate_script": "default"
      }
    },
    "dock": "bottom", // bottom on chaos
    "font_family": "MonoLisa Custom",
    "font_size": 11, // 12 on chaos
    "font_weight": 400,
    "line_height": "comfortable",
    "toolbar": {
      "breadcrumbs": true
    }
  },
  "theme": {
    "dark": "Catppuccin Mocha - No Italics",
    "light": "Catppuccin Frapp√© - No Italics",
    "mode": "system"
  },
  "ui_font_family": "Berkeley Mono",
  "ui_font_features": {
    "calt": false
  },
  "ui_font_size": 13, // 14 on chaos
  "ui_font_weight": 400,
  "unnecessary_code_fade": 0.8,
  "vim": {
    "toggle_relative_line_numbers": true,
    "use_multiline_find": false,
    "use_smartcase_find": false,
    "use_system_clipboard": "always"
  },
  "vim_mode": true,
  "wrap_guides": [80]
}
