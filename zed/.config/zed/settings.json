{
  "assistant": {
    "default_model": {
      "model": "claude-3-5-sonnet-latest",
      "provider": "zed.dev"
    },
    "version": "2"
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "MonoLisa Custom",
  "buffer_font_size": 14,
  "buffer_font_weight": 500,
  "buffer_line_height": "comfortable",
  "cursor_blink": false,
  "cursor_shape": "block",
  "features": {
    "inline_completion_provider": "supermaven"
  },
  "indent_guides": {
    "active_line_width": 1,
    "background_coloring": "disabled",
    "coloring": "indent_aware",
    "enabled": false,
    "line_width": 1
  },
  "project_panel": {
    "default_width": 320,
    "indent_guides": {
      "show": "never"
    }
  },
  "show_whitespaces": "all",
  "show_wrap_guides": true,
  "tab_size": 2,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "font_family": "MonoLisa Custom",
    "font_size": 13,
    "font_weight": 400,
    "line_height": "comfortable"
  },
  "theme": {
    "dark": "Catppuccin Mocha - No Italics",
    "light": "Catppuccin Frapp√© - No Italics",
    "mode": "system"
  },
  "ui_font_family": "Berkeley Mono",
  "ui_font_features": {
    "calt": false
  },
  "ui_font_size": 14,
  "ui_font_weight": 400,
  "vim": {
    "toggle_relative_line_numbers": true,
    "use_multiline_find": false,
    "use_smartcase_find": false,
    "use_system_clipboard": "always"
  },
  "vim_mode": true,
  "wrap_guides": [80]
}
